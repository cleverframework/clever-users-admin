{
  "name": "users-admin",
  "version": "0.1.0",
  "visible": false,
  "author": "Jacopo Daeli <jacopo.daeli@gmail.com>",
  "clever": "0.1.x",
  "scripts": {
    "build": "npm run build-js && npm run build-css",
    "watch": "npm run watch-js && npm run watch-css",
    "build-js": "browserify assets/src/js/index.js | uglifyjs > assets/dist/index.js",
    "watch-js": "watchify assets/src/js/index.js -o assets/dist/index.js --debug --verbose",
    "watch-css": "sourdough -w -v assets/src/styl/index.styl assets/dist/index.css",
    "build-css": "sourdough assets/src/styl/index.styl assets/dist/index.css"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "babelify"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.2.0",
    "bootstrap": "^3.3.6",
    "cookies-js": "^1.2.2",
    "express": "^4.13.4",
    "history": "^1.13.0",
    "isomorphic-fetch": "^2.1.1",
    "jquery": "^2.1.4",
    "react": "^0.14.2",
    "react-dom": "^0.14.0",
    "react-mixin": "^2.0.2",
    "react-redirect": "^1.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.0",
    "redux-logger": "^2.0.2",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^0.1.0"
  },
  "devDependencies": {
    "babelify": "^6.1.0",
    "browserify": "^11.2.0",
    "browserify-shim": "^3.8.10",
    "sourdough-preprocessor": "^1.0.0",
    "uglify-js": "^2.4.15",
    "watchify": "^2.1.1"
  }
}
